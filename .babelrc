{
  "plugins": [
    "babel-plugin-polished",
    [
      "module-resolver",
      {
        "alias": {
          "components": "./components"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "displayName": true
          }
        ]
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "displayName": true,
            "pure": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel"
      ]
    }
  }
}