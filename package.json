{
  "name": "styled-components-docs",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "node server.js",
    "prebuild": "rimraf .next",
    "build": "NODE_ENV=production next build",
    "deploy": "now -t $NOW_TOKEN -T styledcomponents && now alias -t $NOW_TOKEN -T styledcomponents",
    "lint:js": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint './static/**/*.js' './pages/**/*.js' './test/**/*.js' './utils/**/*.js' './components/**/*.js' './sections/**/*.js'",
    "lint": "run-s lint:js lint:css",
    "start": "NODE_ENV=production node server.js",
    "test": "jest -c .jest.config.js",
    "precommit": "lint-staged",
    "prettier": "prettier",
    "size": "bundlesize"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.4",
    "@fortawesome/free-brands-svg-icons": "5.3.1",
    "@fortawesome/free-solid-svg-icons": "5.3.1",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@mdx-js/mdx": "^0.15.0",
    "@mdx-js/runtime": "^0.15.0",
    "@mdx-js/tag": "^0.15.0",
    "@probablyup/react-live": "^1.11.0",
    "@types/react": "^16.4.13",
    "@zeit/next-mdx": "^1.1.1-canary.2",
    "@zeit/next-source-maps": "^0.0.2",
    "axios": "^0.18.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.6.2",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "invariant": "^2.2.4",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.1.3",
    "next": "^6.0.0",
    "polished": "^2.0.3",
    "prismjs": "^1.6.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-octicons-svg": "^1.1.13",
    "styled-components": "4.0.0-beta.8",
    "styled-theming": "^2.2.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "bundlesize": "^0.17.0",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.2.0",
    "lint-staged": "^7.2.2",
    "now": "^11.4.1",
    "npm-run-all": "^4.1.3",
    "prettier": "prettier/prettier",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.6.2",
    "stylelint": "^8.0.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^0.2.1",
    "stylint-stylish": "^1.4.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "prismjs": "1.8.4"
  },
  "lint-staged": {
    "*.js": [
      "jest -c .jest.config.js --findRelatedTests",
      "stylelint",
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --parser mdx --write",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": ".next/bundles/pages/**/*.js",
      "maxSize": "50kB"
    },
    {
      "path": ".next/static/**/*.js",
      "maxSize": "200kB"
    }
  ]
}
