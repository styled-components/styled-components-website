{
  "name": "styled-components-docs",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "sideEffects": [
    "./utils/prismTemplateString.js"
  ],
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
