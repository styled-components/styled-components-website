{
  "name": "styled-components-docs",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "sideEffects": [
    "./utils/prismTemplateString.js"
  ],
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "dev": "node server.js",
    "prebuild": "rimraf .next",
    "build": "NODE_ENV=production next build",
    "deploy": "now -t $NOW_TOKEN -T styledcomponents && now alias -t $NOW_TOKEN -T styledcomponents",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "start": "NODE_ENV=production node server.js",
    "test": "jest -c .jest.config.js",
    "precommit": "lint-staged",
    "prettier": "prettier **/*.js --write",
    "size": "bundlesize"
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.16.0",
    "@mdx-js/runtime": "^0.16.0",
    "@mdx-js/tag": "^0.16.0",
    "@probablyup/react-live": "^1.11.0",
    "@types/react": "^16.7.3",
    "@weco/next-plugin-transpile-modules": "^0.0.2",
    "@zeit/next-mdx": "^1.2.0",
    "@zeit/next-source-maps": "^0.0.3",
    "axios": "^0.18.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-polished": "^1.1.0",
    "compression": "^1.7.3",
    "docsearch.js": "2.6.1",
    "express": "^4.16.4",
    "invariant": "^2.2.4",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.1.3",
    "next": "^7.0.2",
    "polished": "^2.3.0",
    "prismjs": "^1.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1",
    "styled-icons": "^3.7.0",
    "styled-theming": "^2.2.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "bundlesize": "^0.17.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.0.4",
    "now": "^11.4.6",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "lint-staged": {
    "*.js": [
      "jest -c .jest.config.js --findRelatedTests",
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --parser mdx --no-semi --write",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": ".next/static/*/pages/**/*.js",
      "maxSize": "150kB"
    }
  ]
}
