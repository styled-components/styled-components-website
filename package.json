{
  "name": "styled-components-docs",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "deploy": "now -t $NOW_TOKEN -T styledcomponents && now alias -t $NOW_TOKEN -T styledcomponents",
    "lint:js": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint './static/**/*.js' './pages/**/*.js' './test/**/*.js' './utils/**/*.js' './components/**/*.js' './sections/**/*.js'",
    "lint": "run-s lint:js lint:css",
    "start": "NODE_ENV=production node server.js",
    "test": "jest -c .jest.config.js",
    "preintegration": "next build",
    "integration": "jest -c .jest-integration.config.js --forceExit --silent --runInBand",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@zeit/next-preact": "^0.0.5",
    "axios": "^0.17.1",
    "commonmark": "^0.28.1",
    "commonmark-react-renderer": "^4.3.4",
    "express": "^4.15.2",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.0.2",
    "module-alias": "^2.0.0",
    "next": "^5.0.0",
    "polished": "^1.3.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prismjs": "^1.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-live": "^1.9.2",
    "react-octicons-svg": "^1.1.13",
    "styled-components": "^3.1.6",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.2.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "glob": "^7.1.2",
    "glob-promise": "^3.3.0",
    "husky": "^0.14.3",
    "jest": "^22.2.2",
    "jest-image-snapshot": "2.2.0",
    "jest-styled-components": "^4.11.0-0",
    "lint-staged": "^4.0.2",
    "now": "^11.2.5",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "puppeteer": "0.13.0",
    "react-test-renderer": "^16.2.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^0.2.1",
    "stylint-stylish": "^1.4.0",
    "sw-precache-webpack-plugin": "^0.11.4"
  },
  "lint-staged": {
    "*.js": [
      "jest -c .jest.config.js --findRelatedTests",
      "stylelint",
      "eslint --fix",
      "git add"
    ]
  }
}
